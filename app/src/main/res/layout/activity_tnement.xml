<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:background="@color/white"
    >
    <include
        android:id="@+id/include_title"
        layout="@layout/item_title"

        ></include>
    <LinearLayout
        android:layout_below="@+id/include_title"
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:padding="10dp">

        <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <android.support.v7.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/white">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:padding="@dimen/x5">
                    <ImageView
                        android:id="@+id/iv_cell_image"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/y100"
                        android:src="@mipmap/zufang"

                        />
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"


                        >
                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"


                            >
                            <TextView
                                android:layout_width="@dimen/y20"
                                android:layout_height="@dimen/x15"
                                android:background="@drawable/textview_round_border"
                                android:textColor="@color/white"
                                android:gravity="center"
                                android:text="空房"
                                android:textSize="11sp"
                                android:textStyle="bold"

                                />
                            <TextView

                                android:layout_marginLeft="@dimen/x5"
                                android:layout_height="wrap_content"
                                android:layout_width="wrap_content"
                                style="@style/head_title_style"
                                android:textStyle="bold"
                                android:id="@+id/tv_cell_remain"
                                android:text="201"
                                android:textSize="11sp"


                                />
                        </LinearLayout>

                     <LinearLayout
                         android:layout_width="match_parent"
                         android:layout_height="wrap_content"
                         android:orientation="vertical">
                         <LinearLayout
                             android:layout_width="match_parent"
                             android:layout_height="wrap_content"
                             android:orientation="horizontal"
                             android:layout_marginTop="@dimen/x5"
                             >
                             <TextView
                                 android:id="@+id/tv_kongtiao"

                                 style="@style/conner_text_style"


                                 android:layout_width="wrap_content"
                                 android:layout_height="wrap_content"
                                 android:text="空调"


                                 android:textSize="11sp"
                                 />
                             <TextView
                                 style="@style/conner_text_style"

                                 android:id="@+id/tv_chuang"

                                 android:layout_width="wrap_content"
                                 android:layout_height="wrap_content"
                                 android:layout_marginLeft="5dp"

                                 android:text="床"
                                 android:padding="2dp"
                                 android:background="@drawable/seletor_tv"
                                 android:textSize="11sp"
                                 />
                             <TextView
                                 style="@style/conner_text_style"
                                 android:id="@+id/tv_reshuiqi"


                                 android:layout_width="wrap_content"
                                 android:layout_height="wrap_content"
                                 android:layout_marginLeft="5dp"

                                 android:text="热水器"
                                 android:padding="2dp"
                                 android:background="@drawable/seletor_tv"
                                 android:textSize="11sp"
                                 />
                             <TextView
                                 style="@style/conner_text_style"
                                 android:id="@+id/tv_xiyiji"

                                 android:layout_width="wrap_content"
                                 android:layout_height="wrap_content"
                                 android:layout_marginLeft="5dp"

                                 android:text="洗衣机"
                                 android:padding="2dp"
                                 android:background="@drawable/seletor_tv"
                                 android:textSize="11sp"
                                 />
                             <TextView
                                 style="@style/conner_text_style"

                                 android:id="@+id/tv_dianshiji"
                                 android:layout_width="wrap_content"
                                 android:layout_height="wrap_content"
                                 android:layout_marginLeft="5dp"

                                 android:text="电视机"
                                 android:padding="2dp"
                                 android:background="@drawable/seletor_tv"
                                 android:textSize="11sp"
                                 />
                             <TextView
                                 style="@style/conner_text_style"

                                 android:id="@+id/tv_reshuqii"
                                 android:layout_width="wrap_content"
                                 android:layout_height="wrap_content"
                                 android:layout_marginLeft="5dp"

                                 android:text="热水器"
                                 android:padding="2dp"
                                 android:background="@drawable/seletor_tv"
                                 android:textSize="11sp"
                                 />
                             <TextView
                                 style="@style/conner_text_style"

                                 android:id="@+id/tv_yigui"
                                 android:layout_width="wrap_content"
                                 android:layout_height="wrap_content"
                                 android:layout_marginLeft="5dp"

                                 android:text="衣柜"
                                 android:padding="2dp"
                                 android:background="@drawable/seletor_tv"
                                 android:textSize="11sp"
                                 />
                             <TextView
                                 style="@style/conner_text_style"

                                 android:id="@+id/tv_shafa"
                                 android:layout_width="wrap_content"
                                 android:layout_height="wrap_content"
                                 android:layout_marginLeft="5dp"

                                 android:text="沙发"
                                 android:padding="2dp"
                                 android:background="@drawable/seletor_tv"
                                 android:textSize="11sp"
                                 />

                         </LinearLayout>
                         <LinearLayout
                             android:layout_width="match_parent"
                             android:layout_height="wrap_content"
                             android:orientation="horizontal"
                             android:layout_marginTop="5dp">
                             <TextView
                                 style="@style/conner_text_style"
                                 android:id="@+id/tv_huangdai"
                                 android:layout_width="wrap_content"
                                 android:layout_height="wrap_content"
                                 android:text="宽带"
                                 android:textSize="11sp" />
                             <TextView
                                 style="@style/conner_text_style"

                                 android:id="@+id/tv_tianranqi"
                                 android:layout_width="wrap_content"
                                 android:layout_height="wrap_content"
                                 android:layout_marginLeft="5dp"
                                 android:text="天然气"
                                 android:padding="2dp"
                                 android:background="@drawable/seletor_tv"
                                 android:textSize="11sp" />
                             <TextView
                                 style="@style/conner_text_style"
                                 android:id="@+id/tv_bingxiang"
                                 android:layout_width="wrap_content"
                                 android:layout_height="wrap_content"
                                 android:layout_marginLeft="5dp"
                                 android:text="冰箱"
                                 android:padding="2dp"
                                 android:background="@drawable/seletor_tv"
                                 android:textSize="11sp" />


                         </LinearLayout>
                     </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:layout_marginTop="@dimen/x5"
                            >
                            <TextView

                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="¥ "
                                android:textColor="@color/red"

                                android:textSize="10sp"
                                />
                            <TextView
                                android:id="@+id/tv_cell_cost"

                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="280"
                                android:textColor="@color/black"
                                android:textStyle="bold"

                                android:textSize="10sp"
                                />
                            <TextView

                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="/月"
                                android:textColor="@color/black"

                                android:textSize="8sp"
                                />
                        </LinearLayout>
                      <LinearLayout
                          android:layout_width="wrap_content"
                          android:layout_height="wrap_content"
                          android:orientation="horizontal"
                          android:layout_marginTop="@dimen/x5"
                          >
                       <ImageView
                           android:layout_gravity="center"
                           android:layout_width="6dp"
                           android:layout_height="6dp"
                           android:background="@drawable/item_cicle"
                           />
                          <TextView
                              android:id="@+id/tv_style"
                              android:layout_gravity="center"
                              android:layout_width="wrap_content"
                              android:layout_height="wrap_content"
                              android:text="一按一组"
                              android:textColor="@color/red"
                              android:layout_marginLeft="5dp"


                              android:textSize="10sp"
                              />

                      </LinearLayout>
                    </LinearLayout>
                </LinearLayout>

            </android.support.v7.widget.CardView>
        </LinearLayout>


        <View
            android:layout_marginTop="20dp"
            style="@style/match_line"/>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="80dp"
            android:orientation="vertical"
            android:id="@+id/ll_hetong"
            >

            <LinearLayout
                android:id="@+id/ll_details"
                android:layout_width="match_parent"
                android:orientation="horizontal"
                android:gravity="center_vertical"
                style="@style/click"
                android:layout_height="55dp">

                <TextView
                    android:gravity="center_vertical"
                    android:text="合同订约"

                    android:textSize="17sp"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="wrap_content" />
                <ImageView
                    android:src="@mipmap/right_return"
                    android:layout_width="25dp"
                    android:layout_height="25dp" />
            </LinearLayout>
            <View
                style="@style/match_line"/>

    </LinearLayout>
    </LinearLayout>
</RelativeLayout>