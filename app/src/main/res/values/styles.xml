<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="match_line">
        <item name="android:background">@android:color/darker_gray</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1px</item>
    </style>

    <style name="me_item_rl_style" parent="click">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">80dp</item>
        <item name="android:gravity">center_vertical</item>
    </style>
    <style name="click" >
        <item name="android:foreground">?android:attr/selectableItemBackground</item>
    </style>

    <style name="me_text_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:textSize">17sp</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="edit_pro_text_style">
        <item name="android:textColor">@color/txt_black</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:textSize">17sp</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="room_text_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:textSize">17sp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/black</item>
    </style>
    <style name="head_title_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:textSize">20sp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textStyle">bold</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="me_img_style">
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_width">20dp</item>
        <item name="android:layout_height">20dp</item>
    </style>

    <style name="time_dialog" parent="android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowBackground">@color/background</item>
    </style>

    <style name="setting_color_bar_style">
        <item name="android:background">#279e76</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="setting_switch_style">
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="login_text_style">

        <item name="android:textSize">18sp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/gren_cut_clorr</item>
    </style>

    <style name="smell_text_style">
    <item name="android:textSize">11sp</item>


    </style>
    <style name="login_text_style_nor">

        <item name="android:textSize">18sp</item>
        <item name="android:layout_height">wrap_content</item>

    </style>
    <style name="btn_stly">

        <item name="android:textSize">13sp</item>
        <item name="android:layout_height">wrap_content</item>
         <item name="android:background">@drawable/selector_btn_green</item>
        <item name="android:textColor">@color/white</item>

    </style>
</resources>
