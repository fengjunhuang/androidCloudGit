<resources>


    <style name="match_line">
        <item name="android:background">@android:color/darker_gray</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1px</item>
    </style>
    <style name="match_line_dp">
        <item name="android:background">@android:color/black</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1px</item>
    </style>

    <style name="me_item_rl_style" parent="click">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">80dp</item>
        <item name="android:gravity">center_vertical</item>
    </style>
    <style name="click" >
        <item name="android:foreground">?android:attr/selectableItemBackground</item>
    </style>

    <style name="me_text_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:textSize">15sp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/black</item>
    </style>
    <style name="edit_pro_text_style">
        <item name="android:textColor">@color/txt_black</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:textSize">17sp</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="room_text_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:textSize">17sp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/black</item>
    </style>
    <style name="head_title_style" parent="click">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:textSize">20sp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textStyle">bold</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="me_img_style">
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_width">20dp</item>
        <item name="android:layout_height">20dp</item>
    </style>

    <style name="time_dialog" parent="android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowBackground">@color/background</item>
    </style>

    <style name="setting_color_bar_style">
        <item name="android:background">#279e76</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="setting_switch_style">
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="login_text_style">

        <item name="android:textSize">18sp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/gren_cut_clorr</item>
    </style>

    <style name="smell_text_style">
    <item name="android:textSize">11sp</item>


    </style>
    <declare-styleable name="LoadingLayout">
    <attr name="loadingView" format="reference" />
    <attr name="stateView" format="reference" />
    <attr name="emptyView" format="reference" />
</declare-styleable>
    <style name="AnimDown" parent="@android:style/Animation">

        <item name="android:windowEnterAnimation">@anim/push_scale_in</item>

        <item name="android:windowExitAnimation">@anim/push_scale_out</item>

    </style>
    <style name="login_text_style_nor">

        <item name="android:textSize">18sp</item>
        <item name="android:layout_height">wrap_content</item>

    </style>
    <style name="btn_stly">
        <item name="android:textSize">17sp</item>
        <item name="android:layout_height">wrap_content</item>
         <item name="android:background">@drawable/selector_btn_green</item>
        <item name="android:textColor">@color/white</item>
    </style>
    <style name="tv_stly">
        <item name="android:textSize">10sp</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="Dialog_Fullscreen">
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowNoTitle">true</item>
    </style>
    <style name="sensor_text_style">
        <item name="android:textSize">17sp</item>
        <item name="android:layout_marginTop">5dp</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
    </style>
    <style name="conner_text_style">
        <item name="android:textSize">11sp</item>
           <item name="android:padding">2dp</item>
         <item name="android:background">@drawable/seletor_tv</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
    </style>
    <style name="sensor_image_style">
        <item name="android:padding">9dp</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="contract_text_style">
        <item name="android:textSize">13sp</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_marginRight">5dp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:layout_width">20dp</item>
        <item name="android:layout_height">20dp</item>
    </style>



    <style name="CustomDialogStyle" parent="@android:style/Theme.Dialog">
        <!-- 边框 -->
        <item name="android:windowFrame">@null</item>

        <!-- 背景透明 -->
        <item name="android:windowBackground">@color/transparent</item>

        <!-- 无标题 -->
        <item name="android:windowNoTitle">true</item>

        <!-- 是否浮现在activity之上 -->
        <item name="android:windowIsFloating">true</item>

        <!-- 半透明 -->
        <item name="android:windowIsTranslucent">false</item>

        <!-- 背景模糊 -->
        <item name="android:windowContentOverlay">@null</item>

        <!-- 允许对话框的背景变暗 -->
        <item name="android:backgroundDimEnabled">true</item>

        <!-- 字体颜色 -->
        <!--<item name="android:textColor">@color/blue</item>
        <item name="android:editTextColor">@color/blue</item>-->
    </style>

    <style name="tnement_image_style">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_height">@dimen/x30</item>
        <item name="android:padding">10dp</item>
        <item name="android:layout_marginRight">@dimen/x5</item>
    </style>

    <style name="tenment_icon_ll_style">
        <item name="android:paddingRight">@dimen/x18</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="commit_id_tv_style">
        <item name="android:layout_marginTop">20dp</item>
        <item name="android:textColor">@color/light_black</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="style_pop_animation">
    <item name="android:windowEnterAnimation">@anim/push_scale_in</item>
    <item name="android:windowExitAnimation">@anim/push_scale_out</item>
</style>


        <!-- Base application theme. -->
        <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
            <!-- Customize your theme here. -->
            <item name="colorPrimary">@color/colorPrimary</item>
            <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
            <item name="colorAccent">@color/colorAccent</item>
        </style>

        <style name="AppTheme.NoActionBar" parent="Theme.AppCompat.Light.NoActionBar">
            <!-- Customize your theme here. -->
            <item name="colorPrimary">@color/colorPrimary</item>
            <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
            <item name="colorAccent">@color/colorAccent</item>
        </style>

        <style name="AppTheme.Translucent" parent="AppTheme.NoActionBar">
            <item name="android:windowIsTranslucent">true</item>
        </style>

